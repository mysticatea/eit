{
  "name": "eit",
  "version": "0.0.0",
  "description": "Enhanced ITerators.",
  "engines": {
    "node": ">=6"
  },
  "main": "index",
  "files": [
    "index.*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/eslint": "^4.16.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^4.19.1",
    "eslint-config-mysticatea": "^13.0.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-watch": "^4.3.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2",
    "typescript-eslint-parser": "^14.0.0"
  },
  "scripts": {
    "_mocha": "_mocha --require ts-node/register test/**/*.ts --reporter dot",
    "prebuild": "npm run -s clean",
    "build": "run-s build:*",
    "build:inject": "ts-node scripts/generate-src-inject",
    "build:tsc": "tsc --module es2015",
    "build:js": "rollup -c rollup-js.config.js",
    "build:mjs": "rollup -c rollup-mjs.config.js",
    "build:dts": "dts-bundle --name eit --main .temp/index.d.ts --out ../index.d.ts",
    "clean": "rimraf .temp index.*",
    "test": "npm run _mocha",
    "watch": "npm run _mocha -- --watch-extensions ts --watch --growl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysticatea/eit.git"
  },
  "keywords": [],
  "author": "Toru Nagashima (https://github.com/mysticatea)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mysticatea/eit/issues"
  },
  "homepage": "https://github.com/mysticatea/eit#readme"
}
